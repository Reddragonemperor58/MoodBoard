{
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "env": {
    "VITE_SKIP_TESTS": "true"
  },
  "build": {
    "env": {
      "VITE_SKIP_TESTS": "true"
    }
  }
}
